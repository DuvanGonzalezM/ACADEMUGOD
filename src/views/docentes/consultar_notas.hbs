<div class="container text-center">
    <a href="/docentes/consultar/materias" type="button" class="btn btn-danger backBtn">
        <i class="bi bi-arrow-left"></i>
        Atrás
    </a>
    <h4 class="mb-5">Estudiantes de {{materia.[0].nombre}}</h4>
    <div class="row">
        <table id="materias" class="table table-striped table-bordered acade-table" style="width:100%" border="3">
            <thead>
                <tr>
                    <th>Identificación</th>
                    <th>Nombre</th>
                    <th>Primer corte</th>
                    <th>Segundo corte</th>
                    <th>Tercer corte</th>
                    <th>Promedio</th>
                </tr>
            </thead>
            <tbody>
                {{#each estudiantes}}
                <tr>
                    <td>{{numero_estudiante}}</td>
                    <td>{{nombre}} {{apellido}}</td>
                    <td>{{nota_c1}}</td>
                    <td>{{nota_c2}}</td>
                    <td>{{nota_c3}}</td>
                    <td>{{promedio}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
            <div class="d-flex justify-content-center align-items-center mt-0 mb-1">
  <a href="/docentes/consultar/materias" type="button" class="btn btn-danger">
    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-left-circle-fill" viewBox="0 0 16 16">
      <path d="M8 0a8 8 0 1 0 0 16A8 8 0 0 0 8 0m3.5 7.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5z"/>
    </svg>  Atrás
  </a>
</div>
    </div>
</div>