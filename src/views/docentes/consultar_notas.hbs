<div class="container text-center">
    <a href="/docentes/consultar/materias" type="button" class="btn btn-danger backBtn">
        <i class="bi bi-arrow-left"></i>
        Atrás
    </a>
    <h4 class="mb-5">Estudiantes de {{materia.[0].nombre}}</h4>
    <div class="row">
        <table id="materias" class="table table-striped table-bordered acade-table" style="width:100%" border="3">
            <thead>
                <tr>
                    <th>Identificación</th>
                    <th>Nombre</th>
                    <th>Primer corte</th>
                    <th>Segundo corte</th>
                    <th>Tercer corte</th>
                    <th>Promedio</th>
                </tr>
            </thead>
            <tbody>
                {{#each estudiantes}}
                <tr>
                    <td>{{numero_estudiante}}</td>
                    <td>{{nombre}} {{apellido}}</td>
                    <td>{{nota_c1}}</td>
                    <td>{{nota_c2}}</td>
                    <td>{{nota_c3}}</td>
                    <td>{{promedio}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>