<title>Admin: Cansultar</title>
<table id="consultarEstudiantes" class="table table-striped table-bordered mt-2 acade-table" style="width:100%" border="3">
  <thead>
    <tr>
      <th>Identificación</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Acción</th>
    </tr>
  </thead>
  <tbody>
    {{#each estudiantes}}
    <tr>
      <td>{{numero_estudiante}}</td>
      <td>{{nombre}}</td>
      <td>{{apellido}}</td>
      <td>
        <button type="button" class="btn btn-primary">Editar</button>
        <button type="button" class="btn btn-danger">Eliminar</button>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>

<div class="d-flex justify-content-center align-items-center mt-0 mb-1">
  <a href="/admistrativo/estudiantes" type="button" class="btn btn-danger">
  <i class="bi bi-arrow-left-circle-fill"></i> Atrás</a>
</div>
<div >
  <div class="botones-container">
        <button id="btnConsultar horario">
            <img src="<i class="bi bi-calendar-week"></i>" alt="Cosultar horario" style="height: 20px; vertical-align: middle;"> Consultar horario
        </button>
</div>
</div>