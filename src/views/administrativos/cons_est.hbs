<title>Admin: Consultar</title>
<table id="consultarEstudiantes" class="table table-striped table-bordered mt-2 acade-table" style="width:100%" border="3">
  <thead>
    <tr>
      <th>Identificación</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Acción</th>
    </tr>
  </thead>
  <tbody>
    {{#each estudiantes}}
    <tr>
      <td>{{numero_estudiante}}</td>
      <td>{{nombre}}</td>
      <td>{{apellido}}</td>
      <td>
        <a href="/admistrativo/editarestudiante/{{id_estudiante}}"><i class="bi bi-eye"></i></a>
        <a href="/admistrativo/editarestudiante" type="button" class="btn btn-primary">Editar</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>

<div class="d-flex justify-content-center align-items-center mt-0 mb-1">
  <a href="/admistrativo/estudiantes" type="button" class="btn btn-danger">
  <i class="bi bi-arrow-left-circle-fill"></i> Atrás</a>
</div>
<div >

</div>
</div>