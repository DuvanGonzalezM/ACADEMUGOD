<div class="container text-center">
  <a href="/admistrativo/estudiantes" type="button" class="btn btn-danger backBtn">
      <i class="bi bi-arrow-left"></i>
      Atrás
  </a>
  <h1>Lista estudiantes</h1>
  <table id="consultarEstudiantes" class="table table-striped table-bordered mt-2 acade-table" style="width:100%" border="3">
    <thead>
      <tr>
        <th>Identificación</th>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Acción</th>
      </tr>
    </thead>
    <tbody>
      {{#each estudiantes}}
      <tr>
        <td>{{numero_estudiante}}</td>
        <td>{{nombre}}</td>
        <td>{{apellido}}</td>
        <td>
          <a href="/admistrativo/editarestudiante/{{id_estudiante}}"><i class="bi bi-pencil-square"></i></a>
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>